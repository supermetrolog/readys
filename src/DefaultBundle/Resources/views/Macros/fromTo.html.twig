{% macro fromTo(a, b) %}
    {% if a or b %}
        {% if a and b and a != b %}
            {{ a|number_format(0, '.', ' ') }} - {{ b|number_format(0, '.', ' ') }}
        {% else %}
            {% if a == b %}
                {{ a|number_format(0, '.', ' ') }}
            {% else %}
                {{ a|number_format(0, '.', ' ') }}{{ b|number_format(0, '.', ' ') }}
            {% endif %}
        {% endif %}
    {% endif %}
{% endmacro %}